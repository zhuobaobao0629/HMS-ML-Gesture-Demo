<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="es.voghdev.pdfviewpager"
    android:targetSandboxVersion="2"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="29" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:name="es.voghdev.pdfviewpager.SampleApplication"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:label="@string/app_name"
        android:testOnly="true" >
        <activity
            android:name="es.voghdev.pdfviewpager.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="es.voghdev.pdfviewpager.AssetOnXMLActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name="es.voghdev.pdfviewpager.AssetOnSDActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name="es.voghdev.pdfviewpager.RemotePDFActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name="es.voghdev.pdfviewpager.ZoomablePDFActivityPhotoView"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name="es.voghdev.pdfviewpager.PDFWithScaleActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar" />
        <activity
            android:name="es.voghdev.pdfviewpager.InvalidPdfActivity"
            android:theme="@style/Base.Theme.AppCompat.Light.DarkActionBar" />

        <service
            android:name="es.voghdev.pdfviewpager.library.service.CopyAssetService"
            android:exported="false" >
        </service>
    </application>

</manifest>